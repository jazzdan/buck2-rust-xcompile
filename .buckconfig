[repositories]
root = .
prelude = prelude
toolchains = toolchains
platforms = platforms
none = none

[repository_aliases]
config = prelude
ovr_config = prelude
fbcode = none
fbsource = none
fbcode_macros = none
buck = none

[parser]
target_platform_detector_spec = target:root//...->prelude//platforms:default

[project]
ignore = .git

[build]
execution_platforms = prelude//platforms:default